		<PackageReference Include="DSoft.System.Mvvm" Version="3.4.2406.251" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Humbatt.UI.Toolkit.Desktop\Humbatt.UI.Toolkit.Desktop.csproj" />
	</ItemGroup>

</Project>
